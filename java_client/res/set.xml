<?xml version="1.0" encoding="iso8859-2"?>
<!DOCTYPE panel SYSTEM "res/gameground.dtd">
<panel layout="BorderLayout" border="EmptyBorder(4,4,4,4)"><!--{{{-->
	<hbox>
		<panel background="green">
		</panel>
		<vbox minimumsize="&v_sidebar_width_galaxy;,1" preferredsize="&v_sidebar_width_galaxy;,&v_height;" maximumsize="&v_sidebar_width_galaxy;,2000">
			<scrollpane maximumsize="2000,300" autoscrolls="true" tooltiptext="Current highscores.">
				<table id="_champions" />
			</scrollpane>
			<scrollpane autoscrolls="true" maximumsize="2000,300">
				<list id="_visitors" initclass="javax.swing.DefaultListModel" focusable="false" tooltiptext="People that are visiting this table." />
			</scrollpane>
			<scrollpane autoscrolls="true" maximumsize="2000,400" minimumsize="1,400">
				<textpane id="_logPad" editable="false" focusable="true" />
			</scrollpane>
			<panel maximumsize="&v_sidebar_width_galaxy;,40" layout="BorderLayout">
				<readlineprompt id="_messageInput" focusable="true" editable="true" action="onMessage" constraints="BorderLayout.NORTH" tooltiptext="Enter message You want to send to other players." />
			</panel>
			<hbox>
				<glue maximumsize="1000,1"/>
				<button text="Exit !" action="onExit" tooltiptext="Abandon this match." />
			</hbox>
		</vbox>
	</hbox>
</panel><!--}}}-->
